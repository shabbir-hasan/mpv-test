[Appearance]
AntiAliasFonts=false
ColorScheme=Photons
Font=Terminus,10,-1,5,50,0,0,0,0,0

[Cursor Options]
CustomCursorColor=221,204,187
UseCustomCursorColor=true

[General]
Command=/usr/bin/dtach -A /tmp/weechat -Ez weechat-curses
Environment=TERM=konsole-linux
Name=weechat
Parent=FALLBACK/
ShowTerminalSizeHint=false
StartInCurrentSessionDir=true

[Interaction Options]
AutoCopySelectedText=true
TrimTrailingSpacesInSelectedText=true
UnderlineLinksEnabled=false

[Keyboard]
KeyBindings=linux

[Scrolling]
HistoryMode=0
HistorySize=12000
ScrollBarPosition=2

[Terminal Features]
BidiRenderingEnabled=false
FlowControlEnabled=false
